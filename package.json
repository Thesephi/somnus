{
  "name": "somnus",
  "description": "Minimal, database-agnostic API Framework based on Restify",
  "version": "2.0.0",
  "main": "index.js",
  "scripts": {
    "build": "babel lib --out-dir dist",
    "test": "mocha -R spec test/**/*.js",
    "ex:hw": "node --harmony examples/hello-world | bunyan -o short"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-async-to-generator"
    ]
  },
  "dependencies": {
    "bunyan": "^1.8.1",
    "restify": "^4.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "mocha": "^2.4.5"
  },
  "engines": {
    "node": ">= 6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dklabco/somnus.git"
  },
  "bugs": {
    "url": "https://github.com/dklabco/somnus/issues",
    "email": "gate@dklab.co"
  },
  "author": "Khang Dinh <dk@dklab.co>",
  "license": "MIT",
  "keywords": [
    "somnus",
    "api",
    "web",
    "mbaas",
    "framework",
    "restify"
  ]
}
